{
  "name": "zuro-monorepo",
  "private": true,
  "scripts": {
    "build": "corepack pnpm -r --if-present build",
    "build:registry": "corepack pnpm --filter @zuro/templates build:registry",
    "dev:registry": "corepack pnpm --filter @zuro/templates dev:registry",
    "dev": "corepack pnpm -r --parallel --if-present dev",
    "lint": "corepack pnpm -r --if-present lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "turbo": "latest",
    "typescript": "^5.0.0",
    "prettier": "^3.0.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "version": "0.0.1-beta.0"
}
